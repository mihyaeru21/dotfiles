-bar    # To force the opening brace to always be on the right
-boc    # try to break at all old commas
-cab=1  # break at all commas after => if container is open
-i=4    # Indent level is 4 cols
-kbl=2  # keep all old blank lines
-kis    # --keep-interior-semicolons
-l=128  # Max line width is 128 cols
-ndws   # no --delete-old-whitespace
-nsfs   # no --space-for-semicolon
-otr    # --opening-token-right
-pt=2   # White space
-se     # Errors to STDERR
-st     # Output to STDOUT
-wbb="% + - * / x != == >= <= =~ < > | & **= += *= &= <<= &&= -= /= //= |= >>= ||= .= %= ^= x="
